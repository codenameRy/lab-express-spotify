<div class="container">
    <div class="row">
        {{#each albums}}
        <div class="col-lg-4 artist-container">
            <img src={{this.images.0.url}} alt="">
            {{!-- Bug - Should be [0] --}}
            <div class="artist-info-container">
                <h1>{{this.name}}</h1>
                <button><a href="/tracks/{{this.id}}">View Tracks</a></button>
            </div>
        </div>
        {{/each}}

    </div>
</div>