<div class="container">
    <div class="row">
        {{#each artist}}
        <div class="col-lg-4 artist-container">
            <img src={{this.images.0.url}} alt="">
            {{!-- Bug - Should be [0] --}}
            <div class="artist-info-container">
                <h1>{{this.name}}</h1>
                <button><a href="albums/{{this.id}}">View Albums</a></button>
            </div>
        </div>
        {{/each}}

    </div>
</div>